  </sounds>
  <animations>
    <animation name="ballRebirth">
      <sequence loops="1">
        <alpha ticks="1" target="0.0" profile="linear"/>
        <scale ticks="1" target="0.0" profile="linear"/>
        <parallel loops="1">
          <alpha ticks="20" target="255.0" profile="linear"/>
          <scale ticks="100" target="1.0" profile="linear"/>
        </parallel>
      </sequence>
    </animation>
    <animation name="exitGate">
      <sequence loops="1">
        <parallel loops="1">
          <turn ticks="200" turn="3600.0" profile="easeOutCubic"/>
          <alpha ticks="100" target="255.0" profile="linear"/>
          <scale ticks="150" target="1.0" profile="easeInCirc"/>
        </parallel>
        <forwards ticks="50" forwards="100.0" sideways="0.0" profile="default" finishedMessage="exitGate"/>
      </sequence>
    </animation>
    <animation name="hide">
      <sequence loops="1">
        <alpha ticks="50" target="0.0" profile="linear"/>
      </sequence>
    </animation>
    <animation name="popup120">
      <sequence loops="1">
        <move ticks="50" dx="0.0" dy="120.0" profile="easeOutBack"/>
      </sequence>
    </animation>
    <animation name="popup60">
      <sequence loops="1">
        <move ticks="50" dx="0.0" dy="65.0" profile="easeOutBack"/>
      </sequence>
    </animation>
    <animation name="travelGate">
      <parallel loops="1">
        <scale ticks="200" target="0.25" profile="linear"/>
        <sequence loops="1">
          <alpha ticks="100" target="255.0" profile="linear" finishedMessage="collected"/>
          <alpha ticks="100" target="0.0" profile="linear" finishedMessage="exitGate"/>
        </sequence>
      </parallel>
    </animation>
    <animation name="unwrapping">
      <sequence loops="1">
        <move ticks="30" dx="0.0" dy="0.0" profile="linear"/>
        <alpha ticks="80" target="0.0" profile="linear"/>
      </sequence>
    </animation>
    <animation name="wrapping">
      <sequence loops="1">
        <alpha ticks="80" target="255.0" profile="linear"/>
      </sequence>
    </animation>
  </animations>
